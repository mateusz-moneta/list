{
  "npmScope": "list",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "list": {
      "tags": ["platform:web", "type:application"]
    },
    "list-e2e": {
      "tags": ["platform:web", "type:application"],
      "implicitDependencies": ["list"]
    },
    "data-access-list": {
      "tags": ["platform:web", "type:data-access"]
    },
    "ui": {
      "tags": ["platform:web", "type:ui"]
    },
    "shared": {
      "tags": ["platform:web", "type:shared"]
    }
  }
}
